    "@tanstack/react-query-next-experimental": "^5.52.1",
    "@uidotdev/usehooks": "^2.4.1",
    "clsx": "^2.1.1",
    "i18next": "^23.14.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.1",
    "million": "^2.6.4",
    "next": "^14.2.6",
    "next-usequerystate": "^1.17.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.3",
    "server-only": "^0.0.1",
