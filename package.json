    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.17.0",
    "typed-query-selector": "^2.12.0",
    "typescript": "^5.5.4",
